<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.blackjack_server.HelloController">
   <children>
      <Button fx:id="btStart" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#onStartClick" style="-fx-background-color: black; -fx-border-radius: 20; -fx-background-radius: 20;" text="START" textFill="RED">
         <font>
            <Font name="Arial Black" size="20.0" />
         </font>
      </Button>
      <ListView fx:id="lvList" prefHeight="200.0" prefWidth="200.0" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
